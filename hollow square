def hollow_square(rows):
    for i in range(1,rows+1):
        if(i==1 or i==rows):
            for j in range(1,rows+1):
                print("*",end="")
        else:
            for j in range(1,rows+1):
                if(j==1 or j==rows):
                    print("*",end = "")
                else:
                    print(end = "")
        print()

n=int(input("enter the rows"))
hollow_square(n)
                
