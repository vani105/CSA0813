           
n=int(input("enter the age"))
if n<18 & n<60:
 print("elgible for voting")
else:
    print("not eligible for voting")

n=int(input("enter the marks"))
if n>90 & n<80:
   print("the grade is 'A'")
elif n>80 & n<70:
   print("the grade is 'B'")
elif n>70 & n<60:
    print("the grade is'c'")
elif n>60 & n<50:
    print("the grade is 'D'")
